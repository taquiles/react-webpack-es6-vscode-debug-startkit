{
  "name": "react-webpack-vscode-debug",
  "version": "1.0.0",
  "description": "Boilerplate for Vscode's Debug (server and client side) for React, Babel, Webpack's Projects",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --colors --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taquiles/react-webpack-es6-vscode-debug"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel"
  ],
  "author": "Pedro Galv√£o <taquiles@gmail.com> (http://github.com/taquiles)",
  "license": "CC0-1.0",
  "homepage": "https://github.com/taquiles/react-webpack-es6-vscode-debug",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^2.0.1",
    "eslint": "^3.17.1",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-lodash": "^1.10.3",
    "eslint-plugin-react": "^6.3.0",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "dependencies": {
    "lodash": "^4.16.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "config": {
    "host": "localhost",
    "port": 3000,
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  }
}
